    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="/assets/styles/fontawesome-all.min.css" />
    <link rel="stylesheet" href="/assets/styles/main.css" />
    <noscript><link rel="stylesheet" href="/assets/styles/noscript.css" /></noscript>
    <meta name="generator" content="{{ eleventy.generator }}" />
    <link rel="alternate" type="application/rss+xml" href="{{ site.url }}/feed.xml" />

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5" />
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="theme-color" content="#ffffff" />

    <meta name="description" content="{{ site.description | safe }}" />

    <meta property="og:site_name" content="{{ site.name }}" />
    <meta property="og:title" content="{{ collections.all | setMyCustomOrder | setHeadTitle(page, site.name) | safe }}" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="{{ site.url + page.url }}" />
    <meta property="og:image" content="{{ site.url + '/images/' + site.twitter.image }}" />
    <meta property="og:description" content="{{ site.description | safe }}" />

    <meta name="twitter:card" content="summary" />
    <meta name="twitter:description" content="{{ site.description | safe }}" />
    <meta name="twitter:site" content="@{{ site.twitter.user }}" />
    <meta name="twitter:image" content="{{ site.url + '/images/' + site.twitter.image }}" />

    <link type="text/plain" rel="author" href="{{ site.url }}/humans.txt" />
