{
    "name": "unlimited-text-works-the-4th",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean:develop": "rimraf ./_develop",
        "develop:mix:build": "npx mix",
        "develop:mix:watch": "npx mix watch",
        "develop:eleventy:build": "npx @11ty/eleventy --config=Eleventy.js",
        "develop:eleventy:watch": "npx @11ty/eleventy --config=Eleventy.js --serve --port=8080",
        "develop:build-watch": "npm-run-all --serial clean:develop --parallel develop:mix:watch develop:eleventy:watch",
        "develop:build": "npm-run-all clean:develop --parallel develop:mix:build develop:eleventy:build",
        "develop:watch": "npm-run-all clean:develop --parallel develop:mix:watch develop:eleventy:watch",
        "clean:product": "rimraf ./_production",
        "product:mix:build": "npx mix --production",
        "product:eleventy:build": "cross-env NODE_ENV=production npx @11ty/eleventy --config=Eleventy.js --quiet",
        "product:build": "npm-run-all --serial clean:product product:mix:build product:eleventy:build",
        "product:serve": "cd ./_production && npx serve"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@11ty/eleventy-plugin-directory-output": "^1.0.1",
        "@11ty/eleventy-plugin-rss": "^1.1.2",
        "@quasibit/eleventy-plugin-sitemap": "^2.1.5",
        "cross-env": "^7.0.3",
        "html-escaper": "^3.0.3",
        "html-minifier": "^4.0.0",
        "laravel-mix": "^6.0.43",
        "luxon": "^2.4.0",
        "markdown-it": "^13.0.1",
        "markdown-it-anchor": "^8.6.4",
        "markdown-it-deflist": "^2.1.0",
        "markdown-it-footnote": "^3.0.3",
        "markdown-it-table-of-contents": "^0.6.0",
        "npm-run-all": "^4.1.5",
        "resolve-url-loader": "^5.0.0",
        "rimraf": "^3.0.2",
        "sass": "^1.52.1",
        "sass-loader": "^13.0.0",
        "serve": "^13.0.2",
        "sprintf-js": "^1.1.2"
    }
}
